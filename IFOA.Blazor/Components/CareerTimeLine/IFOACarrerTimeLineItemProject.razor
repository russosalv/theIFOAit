@namespace IFOA.Blazor.Components.CareerTimeLine

<IFOACareerTimeLineItem
    Color="Color.Tertiary"
    Icon="@Icons.Material.Filled.AssignmentTurnedIn"
    Date="@Date"
    HeaderText="@CompanyName"
    HeaderSubText="@JobTitle"
    Text="@JobSummary"
    DetailLink="@DetailLink"
    DetailLinkColor="Color.Tertiary"
/>

@code {
    [Parameter] public string Date { get; set; } = "May 2019 - Aug 2019";
    [Parameter] public string CompanyName { get; set; } = "Project Name";
    [Parameter] public string JobTitle { get; set; } = "Comapny Name";
    [Parameter] public string JobSummary { get; set; } = "Project Summary";
    [Parameter] public string DetailLink { get; set; } = "/";
}